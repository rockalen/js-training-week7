A,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,EAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AACA,cAAA,IAAA,CAAA,SAAA,KAAA,SAAA,EAAA;AACA,YAAA,EAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA,SAJA;AAKA,OANA,EAMA,IANA,CAAA;AAOA,KAtBA;AAuBA,IAAA,UAvBA,sBAuBA,OAvBA,EAuBA;AACA,MAAA,CAAA,YAAA,OAAA,EAAA,CAAA,KAAA,CAAA,MAAA;AACA;AAzBA;AAbA,CAAA","sourcesContent":["<template>\r\n  <div style=\"position: fixed; top: 20px; right: 20px; min-width: 300px; z-index: 11000;\">\r\n    <div\r\n      v-for=\"(item, i) in messages\"\r\n      :id=\"`toast-${i}`\"\r\n      :key=\"i\"\r\n      class=\"toast fade show\"\r\n      role=\"alert\"\r\n      aria-live=\"assertive\"\r\n      aria-atomic=\"true\"\r\n      data-autohide=\"false\"\r\n    >\r\n      <div class=\"toast-header\">\r\n        <div\r\n          class=\"inline-block rounded mr-2\"\r\n          style=\"width: 20px; height: 20px\"\r\n          :class=\"`bg-${ item.status }`\"\r\n        />\r\n        <strong class=\"mr-auto\">慢慢<sub>手做</sub></strong>\r\n        <small>現在</small>\r\n        <button\r\n          type=\"button\"\r\n          class=\"ml-2 mb-1 close\"\r\n          data-dismiss=\"toast\"\r\n          aria-label=\"Close\"\r\n          @click=\"closeToast(`toast-${i}`)\"\r\n        >\r\n          <span aria-hidden=\"true\">&times;</span>\r\n        </button>\r\n      </div>\r\n      <div class=\"toast-body\">\r\n        {{ item.message }}\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n/* global $ */\r\nexport default {\r\n  name: 'Toasts',\r\n  data () {\r\n    return {\r\n      messages: []\r\n    }\r\n  },\r\n  created () {\r\n    const vm = this\r\n    vm.$bus.$on('message:push', (message, status = 'warning') => {\r\n      vm.updateMessage(message, status)\r\n    })\r\n  },\r\n  methods: {\r\n    updateMessage (message, status) {\r\n      const timestamp = Math.floor(new Date() / 1000)\r\n      this.messages.push({\r\n        message,\r\n        status,\r\n        timestamp\r\n      })\r\n      this.removeMessageWithTiming(timestamp)\r\n    },\r\n    removeMessage (num) {\r\n      this.messages.splice(num, 1)\r\n    },\r\n    removeMessageWithTiming (timestamp) {\r\n      const vm = this\r\n      setTimeout(() => {\r\n        vm.messages.forEach((item, i) => {\r\n          if (item.timestamp === timestamp) {\r\n            vm.messages.splice(i, 1)\r\n          }\r\n        })\r\n      }, 5000)\r\n    },\r\n    closeToast (element) {\r\n      $(`#${element}`).toast('hide')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scope>\r\n.message-alert {\r\n  position: fixed;\r\n  max-width: 50%;\r\n  top: 56px;\r\n  right: 20px;\r\n  z-index: 1100;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}